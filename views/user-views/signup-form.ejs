<section>
<h2> Sign Up </h2>

<form id="form" method="post" action="/process-signup">

  <% if (typeof feedbackMessage !== 'undefined'){ %>
      <p class="error-message"> <%=feedbackMessage %></p>
    <% } %>

<label for="email-input"> Email </label>
<input id="email-input" type="email" name="signupEmail" class="form-control">
<br>
<label for="password-input"> Password </label>
<input id="password-input" type="password" name="signupPassword" class="form-control">
<br>
  <button class="form-control"> Sign Up</button>
  <br>
</form>
<section>
